<div class="streams">
	<h2>Live Streams</h2>
	{{#each model.liveStreams as |live|}}	
		<div class="stream">
			<iframe src="{{live.channel}}" frameborder="0" scrolling="no"></iframe>
		</div>
	{{/each}}
</div>

<div class="streams">
	<h2>Upcoming Streams</h2>
	<div class="streams">
	{{#each model.upcomingStreams as |upcoming|}}
		<div class="stream">
			<iframe src="{{live.channel}}" frameborder="0" scrolling="no"></iframe>
		</div>
	{{/each}}
	</div>
</div>
